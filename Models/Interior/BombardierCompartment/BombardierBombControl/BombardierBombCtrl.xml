      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.003 </red>
      <green> 0.003 </green>
      <blue>  0.003 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>Indicators</object-name>
    <emission>
      <factor-prop>systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.15 </red>
      <green> 0.15 </green>
      <blue>  0.15 </blue>
    </emission>
  </animation>

  <model>
    <path>Aircraft/B-25/Models/Interior/BombardierCompartment/BombardierBombControl/BombardierBombCtrl_all-switch.xml</path>
  </model>

</PropertyList>
