<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>BombControlPanel.ac</path>

  <animation>
    <object-name>Cube</object-name>
    <object-name>indicatorLightRight</object-name>
    <object-name>RedSwithcLeft</object-name>
    <object-name>RedSwitchRight</object-name>

  </animation>

  <animation>
    <type>material</type>
    <object-name>Cube</object-name>
    <object-name>indicatorLightRight</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.003 </red>
      <green> 0.003 </green>
      <blue>  0.003 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>indicatorLightRight</object-name>
    <object-name>RedSwithcLeft</object-name>
    <object-name>RedSwitchRight</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.15 </red>
      <green> 0.15 </green>
      <blue>  0.15 </blue>
    </emission>
  </animation>

</PropertyList>
