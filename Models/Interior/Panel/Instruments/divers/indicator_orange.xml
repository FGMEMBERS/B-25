<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>indicator_orange.ac</path>

  <animation>
    <object-name>indicator</object-name>
    <object-name>indicatorLightON</object-name>
    <object-name>indicatorLightOFF</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>indicatorLightON</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.5 </red>
      <green> 0.5 </green>
      <blue>  0.5 </blue>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>indicator</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.005 </red>
      <green> 0.005 </green>
      <blue>  0.005 </blue>
    </emission>
  </animation>

</PropertyList>
