<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>knob.ac</path>

  <animation>
    <!-- objects opaques -->
    <object-name>knob</object-name>
    <!-- objects transparent --> 
    <object-name>increase</object-name>
    <object-name>decrease</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>knob</object-name>
    <emission>
      <red>   0.004 </red>
      <green> 0.004 </green>
      <blue>  0.004 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

</PropertyList>
