<?xml version="1.0" encoding="ISO-8859-1"?>

<PropertyList>

  <path>indicator_green.ac</path>

  <animation>
    <object-name>indicator</object-name>
    <object-name>indicatorLight</object-name>
  </animation>

  <animation>
    <type>material</type>
    <object-name>indicatorLight</object-name>
    <emission>
      <red>   0.15 </red>
      <green> 0.15 </green>
      <blue>  0.15 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

  <animation>
    <type>material</type>
    <object-name>indicator</object-name>
    <emission>
      <red>   0.005 </red>
      <green> 0.005 </green>
      <blue>  0.005 </blue>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
    </emission>
  </animation>

</PropertyList>
