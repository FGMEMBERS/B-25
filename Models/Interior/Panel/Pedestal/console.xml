  <!-- Flaps Lever  -->
  <animation>
    <type>rotate</type>
    <object-name>FlapLever</object-name>
    <property>controls/flight/flaps</property>
    <factor> 45 </factor>
    <center>
      <x-m>  0.341 </x-m>
      <y-m>  0.010 </y-m>
      <z-m> -0.118 </z-m>
