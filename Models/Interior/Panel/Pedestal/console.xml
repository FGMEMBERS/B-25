    <center>
      <x-m> -0.126 </x-m>
      <y-m> -0.020 </y-m>
      <z-m>  0.207 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  -1 </y>
      <z>  0 </z>
    </axis>
  </animation>
  
  <text>
    <name>LabelText</name>
    <type type="string">literal</type>
