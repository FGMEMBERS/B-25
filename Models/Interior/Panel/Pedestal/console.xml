  <!-- Flaps Lever  -->
  <animation>
    <type>rotate</type>
    <object-name>FlapLever</object-name>
    <property>controls/flight/flaps</property>
    <factor> 45 </factor>
    <center>
      <x-m>  0.341 </x-m>
      <y-m>  0.010 </y-m>
      <z-m> -0.118 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>FlapLever</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.flapsDown(1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>controls.flapsDown(0)</script>
        </binding>
      </mod-up>
    </action>
    <action>
      <button>1</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>controls.flapsDown(-1)</script>
      </binding>
      <mod-up>
        <binding>
          <command>nasal</command>
          <script>controls.flapsDown(0)</script>
        </binding>
      </mod-up>
    </action>
  </animation>

  <!-- Magnetos  !!  -->
  <animation>
    <type>rotate</type>
    <object-name>MagnetoL</object-name>
    <property>controls/engines/engine[0]/magnetos</property>
    <factor> -30 </factor>
    <center>
      <x-m> -0.218 </x-m>
      <y-m> -0.018 </y-m>
      <z-m>  0.215 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>MagnetoR</object-name>
    <property>controls/engines/engine[1]/magnetos</property>
    <factor> 30 </factor>
    <center>
      <x-m> -0.218 </x-m>
      <y-m>  0.030 </y-m>
      <z-m>  0.215 </z-m>
    </center>
    <axis>
      <x> 0 </x>
      <y> 0 </y>
      <z> 1 </z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>increaseL</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <binding>
        <condition>
          <less-than>
            <property>controls/engines/engine[0]/magnetos</property>
            <value>3</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/magnetos</property>
        <step>1</step>
      </binding>
      <binding>
        <condition>
          <equals>
            <property>controls/engines/engine[0]/magnetos</property>
            <value>3</value>
          </equals>
        </condition>
        <command>property-assign</command>
        <property>controls/engines/engine[0]/starter</property>
        <value>1</value>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>controls/engines/engine[0]/starter</property>
          <value>0</value>
        </binding>
      </mod-up>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>decreaseL</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <binding>
        <command>property-adjust</command>
        <property>controls/engines/engine[0]/magnetos</property>
        <step>-1</step>
        <min>0</min>
        <max>4</max>
        <wrap>false</wrap>
      </binding>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>increaseR</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <binding>
        <condition>
          <less-than>
            <property>controls/engines/engine[1]/magnetos</property>
            <value>3</value>
          </less-than>
        </condition>
        <command>property-adjust</command>
        <property>controls/engines/engine[1]/magnetos</property>
        <step>1</step>
      </binding>
      <binding>
        <condition>
          <equals>
            <property>controls/engines/engine[1]/magnetos</property>
            <value>3</value>
          </equals>
        </condition>
        <command>property-assign</command>
        <property>controls/engines/engine[1]/starter</property>
        <value>1</value>
      </binding>
      <mod-up>
        <binding>
          <command>property-assign</command>
          <property>controls/engines/engine[1]/starter</property>
          <value>0</value>
        </binding>
      </mod-up>
    </action>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>decreaseR</object-name>
    <visible>false</visible>
    <action>
      <button>0</button>
      <binding>
      <command>property-adjust</command>
        <property>controls/engines/engine[1]/magnetos</property>
        <step>-1</step>
        <min>0</min>
        <max>3</max>
      </binding>
    </action>
  </animation>

  <!-- Landing Ligts -->
  <animation>
    <type>material</type>
    <object-name>LeftLandingLight</object-name>
    <object-name>RightLandingLight</object-name>
    <emission>
      <red>   0.25 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>LeftLandingLight</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/lights/landing/state</property>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>LeftLandingLight</object-name>
        <property>sim/model/lights/landing/state</property>
    <factor> 40 </factor>
    <center>
      <x-m> -0.126 </x-m>
      <y-m> -0.042 </y-m>
      <z-m>  0.207 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y>  -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>RightLandingLight</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>property-toggle</command>
        <property>sim/model/lights/landing[1]/state</property>
      </binding>
    </action>
  </animation>
  
  <animation>
    <type>rotate</type>
    <object-name>RightLandingLight</object-name>
    <property>sim/model/lights/landing[1]/state</property>
