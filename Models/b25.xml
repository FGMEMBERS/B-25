  <!-- Les moteurs -->
  <!-- Gauche -->
  <model>
    <path>Aircraft/B-25/Models/Engines/engine.xml</path>
    <offsets>
      <x-m> -3.679 </x-m>
      <y-m> -2.891 </y-m>
      <z-m> -0.115 </z-m>
    </offsets>
  </model>

  <!-- Droit -->
  <model>
    <path>Aircraft/B-25/Models/Engines/engine.xml</path>
    <offsets>
