<?xml version="1.0" encoding="UTF-8"?>

<PropertyList>

  <path>r2600.ac</path>

  <animation>
    <!-- Objets opaques -->
    <object-name>bloc</object-name>
    <object-name>caches</object-name>
    <object-name>cames</object-name>
    <object-name>pistons</object-name>
    <object-name>ecrous</object-name>
    <object-name>bougies</object-name>
    <object-name>tuyaux</object-name>
    <object-name>echappes</object-name>
    <object-name>souflets</object-name>
    <object-name>moteurlow</object-name>
  </animation>

  <animation>
    <name>HighPoly</name>
    <object-name>bloc</object-name>
    <object-name>caches</object-name>
    <object-name>cames</object-name>
    <object-name>pistons</object-name>
    <object-name>ecrous</object-name>
    <object-name>bougies</object-name>
    <object-name>tuyaux</object-name>
    <object-name>souflets</object-name>
    <object-name>echappes</object-name>
  </animation>

  <animation>
    <name>LowPoly</name>
    <object-name>moteurlow</object-name>
  </animation>

  <animation>
    <type>range</type>
    <object-name>HighPoly</object-name>
    <min-m>0</min-m>
    <max-property>/sim/rendering/static-lod/detailed</max-property>
  </animation>

  <animation>
    <type>range</type>
    <object-name>LowPoly</object-name>
    <min-property>/sim/rendering/static-lod/detailed</min-property>
    <max-property>/sim/rendering/static-lod/bare</max-property>
  </animation>

</PropertyList>

